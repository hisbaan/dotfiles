    --interactive-hover: #{{base07-hex}};
    --interactive-active: #{{base07-hex}};
    --interactive-muted: #{{base03-hex}};
    --background-primary: #{{base02-hex}};
    --background-secondary: #{{base01-hex}};
    --background-secondary-alt: #{{base01-hex}};
    --background-tertiary: #{{base01-hex}};
    --background-accent: #{{base04-hex}};
    --background-floating: #{base00-hex};
