    --focus-primary: #{{base0D-hex}};
    --radio-group-dot-foreground: #8ea1e1;
    --guild-header-text-shadow: 0 1px 1px rgba(0,0,0,0.4);
    --channeltextarea-background: #{{base02-hex}};
    --activity-card-background: #{{base01-hex}};
    --textbox-markdown-syntax: #{{base05-hex}};
    --deprecated-card-bg: rgba({{base01-rgb-r}},{{base01-rgb-g}},{{base01-rgb-b}},0.6);
    --deprecated-card-editable-bg: rgba({{base01-rgb-r}},{{base01-rgb-g}},{{base01-rgb-b}},0.3);;
    --deprecated-store-bg: #{{base02-hex}};
    --deprecated-quickswitcher-input-background: #{{base04-hex}};
    --deprecated-quickswitcher-input-placeholder: rgba({{base07-rgb-r}},{{base07-rgb-g}},{{base07-rgb-b}},0.3);
    --deprecated-text-input-bg: rgba(0,0,0,0.1);
    --deprecated-text-input-border: rgba(0,0,0,0.3);
    --deprecated-text-input-border-hover: #040405;
    --deprecated-text-input-border-disabled: #{{base02-hex}};
    --deprecated-text-input-prefix: #{{base07-hex}};
