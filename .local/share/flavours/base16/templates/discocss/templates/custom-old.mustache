    --background-mentioned: rgba({{base09-rgb-r}},{{base09-rgb-g}},{{base09-rgb-b}},0.05);
    --background-mentioned-hover: rgba({{base09-rgb-r}},{{base09-rgb-g}},{{base09-rgb-b}},0.08);
    --background-message-hover: rgba({{base00-rgb-r}},{{base00-rgb-g}},{{base00-rgb-b}},0.07);
    --background-help-warning: rgba({base09-rgb-r}},{{base09-rgb-g}},{{base09-rgb-b}},0.1);
    --background-help-info: rgba({{base0D-rgb-r}},{{base0D-rgb-g}},{{base0D-rgb-b}},0.1);
